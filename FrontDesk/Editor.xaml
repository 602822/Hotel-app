<Window x:Class="FrontDesk.Editor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FrontDesk"
        mc:Ignorable="d"
        Title="Editor" Height="153" Width="800">
    <Grid Height="434" VerticalAlignment="Top">
        <Label Content="ID" HorizontalAlignment="Left" Margin="37,20,0,0" VerticalAlignment="Top"/>
        <Label Content="Check in" HorizontalAlignment="Left" Margin="199,20,0,0" VerticalAlignment="Top"/>
        <Label Content="Check out" HorizontalAlignment="Left" Margin="347,20,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.005,0.525"/>
        <Label Content="Room" HorizontalAlignment="Left" Margin="522,20,0,0" VerticalAlignment="Top"/>
        <TextBox Name="sid" HorizontalAlignment="Left" Margin="0,51,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="scheckin" HorizontalAlignment="Left" Margin="166,51,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="scheckout" HorizontalAlignment="Left" Margin="319,51,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="sroom" HorizontalAlignment="Left" Margin="483,51,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button Name="bAdd" Content="Add" HorizontalAlignment="Left" Margin="148,100,0,0" VerticalAlignment="Top"/>
        <Button Name="bDelete" Content="Delete" HorizontalAlignment="Left" Margin="347,100,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
